def startBoard():
    m=[['W', 'W', 'W', 'W', 'W']]
    m=m*5
    return m
print startBoard()

def showBoard(b):
    result = ""
    for element in b:
        for subelement in element:
            result = result + subelement
        print result
        result = "" 
print showBoard([['W','W','W','W','W'], ['W','W','W','W','W'], ['W','W','W','W','W'], ['W','W','W','W','W'], ['W','W','W','W','W']])

